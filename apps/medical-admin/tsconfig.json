{
  "extends": "config-tsconfig/tsconfig.nextjs.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      ".": [
        "."
      ],
      "apexcharts": [
        "../../../node_modules/apexcharts-clevision"
      ],
      "apexcharts/*": [
        "../../../node_modules/apexcharts-clevision/*"
      ]
    }
  },
  // Files and folders starting with a . are ignored by the glob patterns
  "include": [
    ".",
    ".eslintrc.cjs",
    ".eslintrc.js",
    "../../packages/config-mui/mui-theme.d.ts",
  ],
  "exclude": [
    "node_modules",
    ".next",
    ".turbo",
    "dist",
    "compiled",
    "./server.js"
  ]
}