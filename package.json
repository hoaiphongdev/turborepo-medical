{
  "name": "turborepo-medical",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "tsc": "turbo tsc",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "clean": "npm exec --workspaces -- shx rm -rf node_modules .next out .turbo dist compiled && shx rm -rf node_modules",
    "remove-turbo-cache": "shx rm -rf ./node_modules/.cache/turbo",
    "update-dependencies": "turbo update-dependencies && npx npm-check-updates -u"
  },
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.3",
    "@mui/icons-material": "5.15.2",
    "@mui/material": "5.15.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.15.0",
    "@tanstack/react-query-devtools": "5.15.0",
    "axios": "^1.6.3",
    "clsx": "^2.1.0",
    "date-fns": "3.0.6",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "14.0.4",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-redux": "^9.0.4",
    "redux-logger": "^3.0.6",
    "swiper": "^11.0.5",
    "tailwind-safelist-generator": "^1.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.10.5",
    "@types/nprogress": "^0.2.3",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/redux-logger": "^3.0.12",
    "autoprefixer": "10.4.16",
    "commitizen": "4.3.0",
    "postcss": "8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.10",
    "shx": "0.3.4",
    "tailwindcss": "3.4.0",
    "ts-node": "10.9.2",
    "tsup": "8.0.1",
    "turbo": "1.11.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  }
}
